    <icon src="img/ios/coca.png" gap:platform="ios" width="144" height="144" />
    
    <gap:splash src="img/ios/coca.png" gap:platform="ios" width="320"  height="480"  />
    <gap:splash src="img/ios/coca.png" gap:platform="ios" width="640"  height="960"  />
    <gap:splash src="img/ios/coca.png" gap:platform="ios" width="640"  height="1136" />
    <gap:splash src="img/ios/ios-1024.png" gap:platform="ios" width="1024" height="748"  />
    <gap:splash src="img/ios/ios-1024.png" gap:platform="ios" width="768"  height="1004" />
    <!-- retina iPad -->
    <gap:splash src="img/ios/ios-2048.png" gap:platform="ios" width="2048" height="1496" />
    <gap:splash src="img/ios/ios-2048.png" gap:platform="ios" width="1536" height="2008" />
    
    

    <description>
        Esto es una Prueba para la APP de Hidratacion
    </description>

    <author href="http://www.llorenteycuenca.com" email="vvalverde@llorenteycuenca.com">
        Victor Valverde
    </author>
    
    <!-- Core plugins -->
    <gap:plugin name="org.apache.cordova.battery-status" />
    <gap:plugin name="org.apache.cordova.camera" />
